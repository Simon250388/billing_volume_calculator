<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="Simanov Aleksey">
        <createTable tableName="accounting_point_key_room">
            <column name="id" type="BigInt" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="accounting_point_id" type="bigint">
                <constraints nullable="false" foreignKeyName="fk_accounting_point_key_room_accounting_point_id_buildings" references="accounting_points(id)"/>
            </column>
            <column name="key_room_id" type="bigint">
                <constraints nullable="false" foreignKeyName="fk_accounting_point_key_room_id_key_rooms" references="key_rooms(id)"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
